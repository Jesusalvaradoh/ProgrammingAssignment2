corr <- function(directory, threshold = 0) {
dir <- paste(getwd(), directory, sep = '/')
finalVector <- c()
names <- c("id", "nobs")
result <- data.frame(row.names = names)
for (i in id) {
file <- paste (dir, paste(formatC( i, width = 3, flag = "0"), 'csv', sep = "."), sep = '/')
dataFromFile <- read.csv(file)
ok <- complete.cases(dataFromFile)
completes<-dataFromFile[ok,]
if (threshold > nrow(completes)) {
finalVector <- c(finalVector, cor(dataFromFile$sulfate, dataFromFile$nitrate))
}
else{
finalVector <- c(finalVector, 0)
}
}
finalVector
}
